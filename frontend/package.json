{
  "name": "divemap-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "^10.4.14",
    "axios": "^1.3.4",
    "fuse.js": "^7.1.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "leaflet": "^1.9.4",
    "leaflet-draw": "^1.0.4",
    "leaflet.markercluster": "^1.5.3",
    "lucide-react": "^0.539.0",
    "playwright": "^1.56.1",
    "postcss": "^8.4.31",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-geolocated": "^4.3.0",
    "react-helmet-async": "^2.0.5",
    "react-hot-toast": "^2.6.0",
    "react-leaflet": "^4.2.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^7.8.1",
    "react-scripts": "^5.0.1",
    "react-slider": "^2.0.6",
    "recharts": "^3.2.1",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.2.5",
    "puppeteer": "^24.15.0"
  },
  "scripts": {
    "postinstall": "npx update-browserslist-db@latest || true",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:prod": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "build:with-compression": "npm run build && ./scripts/precompress-assets.sh",
    "test": "react-scripts test",
    "test:e2e": "node tests/run_frontend_tests.js",
    "test:frontend": "node tests/test_frontend.js",
    "test:validation": "node tests/validate_frontend.js",
    "test:mapview": "node tests/test_map_view_and_proptypes.js",
    "test:nodejs20": "node tests/test_nodejs_20_upgrade.js",
    "test:comprehensive": "node tests/run_comprehensive_tests.js",
    "test:all": "npm run test:nodejs20 && npm run test:mapview && npm run test:validation",
    "lint": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,md}\"",
    "lint:check": "node scripts/lint-check.js",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "nth-check": "^2.0.1",
    "svgo": "^3.0.2",
    "postcss": "^8.4.31",
    "webpack-dev-server": "^4.15.1",
    "resolve-url-loader": "^5.0.0"
  }
}
